[user]
  name = Carl HÃ¶rberg
  email = carl.hoerberg@gmail.com
[color]
  ui = true
[core]
  quotepath = false
  editor = /usr/bin/vim
  excludesfile = /Users/carl/.gitignore_global
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  log = log -p
  aa = add -A
  cm = commit -m
  cam = commit -am
  cmp = commit -p
  lsd = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ps = push
  pl = pull
  plr = pull --rebase
[push]
  default = current
	followTags = true
[help]
  autocorrect = 1
[diff]
	algorithm = histogram
